{
  "name": "bolt-fitness-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --verbose",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:dev": "cypress open",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:yaml": "prettier --write \".github/**/*.yml\"",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "format:all": "npm run format && npm run format:yaml",
    "validate": "npm run lint && npm run type-check && npm run test",
    "clean": "rm -rf .next out node_modules",
    "clean:cache": "rm -rf .next",
    "update": "npm update --save --save-dev",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "analyze": "ANALYZE=true next build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "deploy": "vercel deploy --prod",
    "setup": "bash scripts/setup-all.sh",
    "setup:vercel": "bash scripts/setup-vercel.sh",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "lint:fix": "eslint --fix .",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel deploy --prod",
    "vercel:env:pull": "vercel env pull .env.local",
    "vercel:build": "vercel build",
    "vercel-build": "next build",
    "build:ci": "next build",
    "deploy:vercel": "vercel --prod",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:generate-types": "supabase gen types typescript --local > src/types/database.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.yml": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fontsource/montserrat": "^5.0.0",
    "@fontsource/roboto": "^5.0.0",
    "@prisma/client": "^5.0.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@supabase/auth-helpers-nextjs": "^0.7.4",
    "@supabase/supabase-js": "^2.31.0",
    "@tanstack/react-query": "^4.29.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.15.0",
    "jsonwebtoken": "^9.0.1",
    "lucide-react": "^0.263.1",
    "next": "^13.4.12",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.0",
    "recharts": "^2.7.2",
    "socket.io-client": "^4.7.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "zod": "^3.21.4",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.0.0",
    "@playwright/test": "^1.50.1",
    "@storybook/builder-webpack5": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@testing-library/cypress": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cypress": "^13.0.0",
    "eslint": "^8.45.0",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.2",
    "lint-staged": "^15.0.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "supabase": "^1.88.0",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3",
    "whatwg-fetch": "^3.6.20"
  }
}
